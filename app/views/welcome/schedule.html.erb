.ui-tooltip,.qtip{position:absolute;left:-28000px;top:-28000px;display:none;max-width:280px;min-width:60px;width:auto;line-height:12px;border-width:1px;border-style:solid;}
.ui-tooltip-fluid{display:block;visibility:hidden;position:static!important;float:left!important;}
